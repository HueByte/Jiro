# Jiro Kernel Environment Variables
# Copy this file to .env and update the values for your environment

# Core API Configuration
JIRO_ApiKey=your-api-key-here
JIRO_JiroApi=https://localhost:18092
JIRO_TokenizerUrl=http://localhost:8000

# Database Configuration
JIRO_ConnectionStrings__JiroContext=

# OpenAI Configuration
JIRO_Gpt__AuthToken=your-openai-token-here
JIRO_Gpt__BaseUrl=https://api.openai.com/v1/
JIRO_Gpt__Organization=

# WebSocket Configuration
JIRO_WebSocket__ApiKey=your-api-key-here
JIRO_WebSocket__HubUrl=https://localhost:5001/instanceHub

# gRPC Configuration
JIRO_Grpc__ServerUrl=https://localhost:5001

# Application Settings
JIRO_Whitelist=true
JIRO_ASPNETCORE_ENVIRONMENT=Production
JIRO_ASPNETCORE_URLS=http://+:8080;https://+:8443

# Test Mode (for CI/CD)
JIRO_TEST_MODE=false